	private final InventoryRepository inventoryRepository;
	
public List<InventoryResponse> isInStock(List<String> skuCode) {
	List<Inventory> findBySkuCodeIn = inventoryRepository.findBySkuCodeIn(skuCode);
	return null;
	
}
}
